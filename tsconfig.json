{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      140
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "lib": [
    "es2015"
  ]
}
